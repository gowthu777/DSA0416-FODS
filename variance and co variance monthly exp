import numpy as np

expenses = np.array([
    [10, 12, 11, 13, 12],
    [8, 9, 10, 8, 9],
    [15, 17, 16, 18, 17],
    [6, 7, 8, 6, 7]
])

variance_expenses = np.var(expenses, axis=1)
print("Variance of Each Department's Expenses:")
print(variance_expenses)

covariance_matrix = np.cov(expenses)
print("\nCovariance Matrix of Expenses:")
  print(covariance_matrix)
